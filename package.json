{
  "name": "pensador-promise",
  "version": "0.4.6",
  "description": "Frases vindas do Pensador via WebScraping",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:cov": "npm run test:coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/pensador-promise.git"
  },
  "keywords": [
    "pensador-api",
    "node",
    "mongo",
    "express",
    "javascript",
    "server"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/pensador-promise/issues"
  },
  "homepage": "https://pensador-promise.vercel.app/",
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/iconv-lite": "^0.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "18.6.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "4.7.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "iconv-lite": "^0.6.3",
    "qs": "^6.11.0",
    "slugify": "^1.6.5",
    "undici": "^5.8.0"
  }
}
