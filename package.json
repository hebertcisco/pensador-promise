{
  "name": "pensador-promise",
  "version": "0.3.0",
  "description": "Frases vindas do Pensador via WebScraping",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/pensador-promise.git"
  },
  "keywords": [
    "boilerplate",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/pensador-promise/issues"
  },
  "homepage": "https://pensador-promise.vercel.app/",
  "devDependencies": {
    "@types/cheerio": "^0.22.29",
    "@types/iconv-lite": "^0.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "7.0.7",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.0",
    "ts-jest": "^26.4.1",
    "typescript": "4.2.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.9",
    "iconv-lite": "^0.6.3",
    "node-fetch": "^2.6.1",
    "slugify": "^1.5.3"
  }
}